<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension-point id="palette" name="palette" schema="schema/palette.exsd"/>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Rental"
            id="com.magellium.rental.ui.category1">
      </category>
      <view
            name="Rental"
            icon="icons/Rentals.png"
            category="com.magellium.rental.ui.category1"
            class="com.magellium.rental.ui.views.RentalPropertyView"
            restorable="true"
            id="com.magellium.rental.ui.viewrental">
      </view>
      <view
            name="RentalAgencyView"
            icon="icons/Agency.png"
            category="com.magellium.rental.ui.category1"
            class="com.magellium.rental.ui.views.RentalAgencyPropertyView"
            restorable="true"
            id="com.magellium.rental.ui.rentalAgencyView">
      </view>
      <view
            name="Customer"
            icon="icons/Customers.png"
            category="com.magellium.rental.ui.category1"
            class="com.magellium.rental.ui.views.CustomerView"
            restorable="true"
            id="com.magellium.rental.ui.customerView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Rental"
            class="com.magellium.rental.ui.RentalPerspective"
            id="com.magellium.rental.ui.RentalPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="Default"
            category="com.magellium.rental.ui.PalettePreferencePage"
            class="com.magellium.rental.ui.RentalPreferencePage"
            id="com.magellium.rental.ui.RentalPreferencePage">
      </page>
      <page
            name="Rental Palettes"
            class="com.magellium.rental.ui.PalettePreferencePage"
            id="com.magellium.rental.ui.PalettePreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.magellium.rental.ui.RentalPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="HW test">
            <command
                  commandId="com.magellium.rental.ui.HWCommand"
                  label="Hello World"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.magellium.rental.ui.HWCommand"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.magellium.rental.ui.HWCommand"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.magellium.rental.ui.rentalAgencyView?after=additions">
         <command
               commandId="com.magellium.rental.ui.HWCommand"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.magellium.rental.ui.HWCommand"
            class="com.magellium.rental.ui.HelloWorldHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Hello World"
            id="com.magellium.rental.ui.HWCommand">
      </command>
   </extension>
   <extension
         point="com.magellium.rental.ui.palette">
      <palette
            paletteClass="com.magellium.rental.ui.palettes.DefaultPalette"
            name="Default"
            id="com.magellium.rental.ui.palette.default">
      </palette>
      <palette
            paletteClass="com.magellium.rental.ui.palettes.MochePalette"
            name="Moche"
            id="com.magellium.rental.ui.palette2">
      </palette>
   </extension>
   <extension
         id="com.magellium.rental.ui.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="com.opcoach.e4.preferences.e4PreferencePages">
      <page
            category="com.magellium.rental.ui.PalettePreferencePage"
            class="com.magellium.rental.ui.RentalPreferencePage"
            id="com.magellium.rental.ui.RentalPreferencePage"
            name="Default">
      </page>
      <page
            class="com.magellium.rental.ui.PalettePreferencePage"
            id="com.magellium.rental.ui.PalettePreferencePage"
            name="Rental Palettes">
      </page>
   </extension>

</plugin>
